<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center">

    <Button
        android:id="@+id/chapter_2_messenger"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chapter 2 Messenger"
        android:textAllCaps="false"/>

    <Button
        android:id="@+id/chapter_2_aidl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chapter 2 AIDL"
        android:textAllCaps="false"/>

    <Button
        android:id="@+id/chapter_2_provider"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chapter 2 Provider"
        android:textAllCaps="false"/>

    <Button
        android:id="@+id/chapter_2_socket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chapter 2 Socket"
        android:textAllCaps="false"/>

    <Button
        android:id="@+id/chapter_2_binder_pool"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chapter 2 BinderPool"
        android:textAllCaps="false"/>
</LinearLayout>
